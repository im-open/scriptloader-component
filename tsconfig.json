{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "jsx": "react",
    "lib": ["ES2019", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "typeRoots": ["node_modules/@types"],
    "rootDir": "./src",
    "skipLibCheck": true,
    "paths": {
      "scriptloader-support/*": ["./src/scriptloader-support/*"]
    }
  },
  "include": ["src"]
}
